  <tbody class="linkMOUSE"> 
    <% @posts.each do |post| %>   
      <tr data-link="<%= post_path(post) %>">
        <th scope="row"><%= post.replies_count %></th>
        <td><%= post.title %></td>
        <td><% if post.replies.present? %>
            <%= post.last_replied_at %>
            <% end %></td>
        <td><%= icon('fas', 'eye') %><%= post.vieweds_count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<nav class="container">
  <%= paginate @posts %>
</nav>